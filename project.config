[project]
	description = Mirror of the main Trace Compass git repository
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/inf4990-equipe01
[access "refs/for/refs/*"]
	read = group user/inf4990-equipe01
	push = group user/inf4990-equipe01
	label-Code-Review = -2..+2 group user/inf4990-equipe01
	label-Verified = -1..+1 group user/inf4990-equipe01
	rebase = group user/inf4990-equipe01
	removeReviewer = group user/inf4990-equipe01
	submit = group user/inf4990-equipe01
[access "refs/heads/*"]
	read = group user/inf4990-equipe01
	create = group user/inf4990-equipe01
	push = +force group user/inf4990-equipe01
	pushMerge = group user/inf4990-equipe01
[access "refs/tags/*"]
	push = +force group user/inf4990-equipe01
	pushTag = group user/inf4990-equipe01
	pushSignedTag = group user/inf4990-equipe01
